class "Класс для измерения времени";
import classes Functions.Date;

inclass


  var _stamp: date;


  func new: Timer;
    
    result = self.create;
    result.update;

    return result;
  end;

inobject


  func update: Timer;

    self._stamp = date_now;
    return self;
  end;


  func passed: real;
    
    return date_period(self._stamp, date_now);
  end;
end